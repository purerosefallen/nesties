{
  "name": "nesties",
  "description": "Nest.js utilities",
  "version": "1.1.4",
  "main": "dist/index.cjs",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.mjs",
      "require": "./dist/index.cjs",
      "types": "./dist/index.d.ts"
    }
  },
  "scripts": {
    "lint": "eslint --fix .",
    "build": "node build.js",
    "build:cjs": "node build.js cjs",
    "build:esm": "node build.js esm",
    "build:types": "node build.js types",
    "clean": "node build.js clean",
    "test": "jest --passWithNoTests",
    "start": "node dist/index.js"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/purerosefallen/nesties.git"
  },
  "author": "Nanahira <nanahira@momobako.com>",
  "license": "MIT",
  "keywords": [],
  "bugs": {
    "url": "https://github.com/purerosefallen/nesties/issues"
  },
  "homepage": "https://github.com/purerosefallen/nesties",
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "tests",
    "testRegex": ".*\\.spec\\.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "collectCoverageFrom": [
      "**/*.(t|j)s"
    ],
    "coverageDirectory": "../coverage",
    "testEnvironment": "node"
  },
  "devDependencies": {
    "@nestjs/common": "^11.0.14",
    "@nestjs/config": "^4.0.2",
    "@nestjs/core": "^11.1.8",
    "@nestjs/platform-express": "^11.1.8",
    "@nestjs/swagger": "^11.1.1",
    "@nestjs/testing": "^11.1.8",
    "@types/express": "^5.0.5",
    "@types/jest": "^29.5.14",
    "@types/node": "^22.8.1",
    "@types/supertest": "^6.0.3",
    "@typescript-eslint/eslint-plugin": "^6.21.0",
    "@typescript-eslint/parser": "^6.21.0",
    "esbuild": "^0.25.12",
    "esbuild-register": "^3.6.0",
    "eslint": "8.22.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "^5.2.1",
    "fastify": "^5.6.1",
    "jest": "^29.7.0",
    "prettier": "^3.3.3",
    "rxjs": "^7.8.2",
    "supertest": "^7.1.4",
    "ts-jest": "^29.2.5",
    "typescript": "^5.6.3"
  },
  "peerDependencies": {
    "@nestjs/common": "^11.0.0 || ^9.4.0 || ^10.0.0",
    "@nestjs/config": "^4.0.0 || ^3.0.0",
    "@nestjs/core": "^11.0.0 || ^9.4.0 || ^10.0.0",
    "@nestjs/swagger": "^11.0.0 || ^7.1.8 || ^6.3.0",
    "rxjs": ">=7.0.0"
  },
  "dependencies": {
    "nfkit": "^1.0.9"
  }
}
