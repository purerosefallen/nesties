{
  "name": "nesties",
  "description": "Nest.js utilities",
  "version": "1.0.4",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "lint": "eslint --fix .",
    "build": "rimraf dist && tsc",
    "test": "jest --passWithNoTests",
    "start": "node dist/index.js"
  },
  "repository": {
    "type": "git",
    "url": "https://code.mycard.moe/3rdeye/nesties.git"
  },
  "author": "Nanahira <nanahira@momobako.com>",
  "license": "MIT",
  "keywords": [],
  "bugs": {
    "url": "https://code.mycard.moe/3rdeye/nesties/issues"
  },
  "homepage": "https://code.mycard.moe/3rdeye/nesties",
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "tests",
    "testRegex": ".*\\.spec\\.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "collectCoverageFrom": [
      "**/*.(t|j)s"
    ],
    "coverageDirectory": "../coverage",
    "testEnvironment": "node"
  },
  "devDependencies": {
    "@nestjs/common": "^10.4.6",
    "@nestjs/config": "^3.3.0",
    "@nestjs/swagger": "^7.4.2",
    "@types/jest": "^29.5.14",
    "@types/node": "^22.8.1",
    "@typescript-eslint/eslint-plugin": "^6.21.0",
    "@typescript-eslint/parser": "^6.21.0",
    "eslint": "8.22.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "^5.2.1",
    "jest": "^29.7.0",
    "prettier": "^3.3.3",
    "rimraf": "^6.0.1",
    "ts-jest": "^29.2.5",
    "typescript": "^5.6.3"
  },
  "peerDependencies": {
    "@nestjs/common": "^9.4.0 || ^10.0.0",
    "@nestjs/swagger": "^7.1.8 || ^6.3.0",
    "@nestjs/config": "^3.0.0"
  }
}
